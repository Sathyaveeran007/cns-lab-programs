# Simple simulation â€“ how long till all zero lanes become non-zero
state = [[0]*5 for _ in range(5)]
nonzero_rounds = 0

# Assume one non-zero bit per lane per round
for round in range(1, 25):
    for i in range(5):
        for j in range(5):
            if state[i][j] == 0:
                state[i][j] = round
    if all(all(x > 0 for x in row) for row in state):
        nonzero_rounds = round
        break

print("Rounds to fill all lanes:", nonzero_rounds)
